{
  "manifest_version": 2,

  "name": "Hide EndGame & GOT Spoilers",
  "description": "Trying to save you from Avengers: ENDGAME and GOT Spoilers ",
  "version": "1.1",
  "icons": { 
          "128": "avenger.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "endGame.js"],
      "run_at": "document_end"
    }
  ]
}
